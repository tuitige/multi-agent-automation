# Development Environment Configuration
# Copy this to .env.local and fill in your values

# OpenAI API Key for LangGraph agents
OPENAI_API_KEY=your_openai_api_key_here

# MCP Server Configuration
MCP_SERVER_URL=http://localhost:3000
HMAC_SECRET=your_development_hmac_secret

# Zapier Configuration (for MCP Server)
ZAPIER_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/YOUR_HOOK_ID/YOUR_HOOK_KEY
ZAPIER_HMAC_SECRET=your_zapier_hmac_secret

# AWS Configuration (for deployment)
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your_aws_account_id

# Development Settings
NODE_ENV=development
PORT=3000